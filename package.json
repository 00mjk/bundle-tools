{
  "name": "@intlify/bundle-tools",
  "version": "1.0.0",
  "license": "MIT",
  "author": {
    "name": "kazuya kawaguchi",
    "email": "kawakazu80@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/intlify/bundle-tools.git"
  },
  "bugs": {
    "url": "https://github.com/inlitify/bundle-tools/issues"
  },
  "scripts": {
    "clean": "npm-run-all clean:* clean:*:*",
    "clean:lib:rollup-plugin-vue-i18n": "rm -rf ./packages/rollup-plugin-vue-i18n/lib",
    "clean:lib:vite-plugin-vue-i18n": "rm -rf ./packages/vite-plugin-vue-i18n/lib",
    "clean:lib:vue-i18n-loader": "rm -rf ./packages/vue-i18n-loader/lib",
    "clean:cache:jest": "jest --clearCache",
    "clean:coverage": "rm -rf ./coverage",
    "clean:example:rollup-plugin-vue-i18n": "npm-run-all clean:example:rollup-plugin-vue-i18n:*",
    "clean:example:rollup-plugin-vue-i18n:composition": "rm -rf ./packages/rollup-plugin-vue-i18n/examples/composition/index.js",
    "clean:example:rollup-plugin-vue-i18n:global": "rm -rf ./packages/rollup-plugin-vue-i18n/examples/global/index.js",
    "clean:example:rollup-plugin-vue-i18n:legacy": "rm -rf ./packages/rollup-plugin-vue-i18n/examples/legacy/index.js",
    "build:rollup-plugin-vue-i18n": "cd packages/rollup-plugin-vue-i18n && tsc -b",
    "build:vite-plugin-vue-i18n": "cd packages/vite-plugin-vue-i18n && tsc -b",
    "build:vue-i18n-loader": "cd packages/vue-i18n-loader && tsc -b",
    "build:example:rollup-plugin-vue-i18n": "yarn build:rollup-plugin-vue-i18n && npm-run-all --parallel build:example:rollup-plugin-vue-i18n:*",
    "build:example:vite-plugin-vue-i18n": "vite build --config ./packages/vite-plugin-vue-i18n/examples/vite.config.ts --outDir ./packages/vite-plugin-vue-i18n/examples/dist",
    "build:example:vue-i18n-loader": "yarn build:vue-i18n-loader && webpack-dev-server --config ./packages/vue-i18n-loader/examples/webpack.config.js --inline --hot",
    "build:example:rollup-plugin-vue-i18n:composition": "rollup -c ./packages/rollup-plugin-vue-i18n/examples/rollup.config.js --environment BUILD:composition",
    "build:example:rollup-plugin-vue-i18n:global": "rollup -c ./packages/rollup-plugin-vue-i18n/examples/rollup.config.js --environment BUILD:global",
    "build:example:rollup-plugin-vue-i18n:legacy": "rollup -c ./packages/rollup-plugin-vue-i18n/examples/rollup.config.js --environment BUILD:legacy",
    "dev:vite-plugin-vue-i18n": "vite ./packages/vite-plugin-vue-i18n/examples --config ./packages/vite-plugin-vue-i18n/examples/vite.config.ts",
    "dev:vite-plugin-vue-i18n:composition": "vite ./packages/vite-plugin-vue-i18n/examples/composition --config ./packages/vite-plugin-vue-i18n/examples/vite.config.ts",
    "dev:vite-plugin-vue-i18n:global": "vite ./packages/vite-plugin-vue-i18n/examples/global --config ./packages/vite-plugin-vue-i18n/examples/vite.config.ts",
    "dev:vite-plugin-vue-i18n:legacy": "vite ./packages/vite-plugin-vue-i18n/examples/legacy --config ./packages/vite-plugin-vue-i18n/examples/vite.config.ts",
    "fix": "npm-run-all --parallel lint:fix format:fix",
    "lint": "eslint ./packages --ext .ts",
    "lint:fix": "yarn lint --fix",
    "format": "prettier --config .prettierrc --ignore-path .prettierignore '**/*.{js,json,html}'",
    "format:fix": "yarn format --write",
    "release:prepare": "shipjs prepare",
    "release:trigger": "shipjs trigger",
    "coverage": "opener coverage/lcov-report/index.html",
    "test": "yarn lint && yarn test:cover && yarn test:e2e",
    "test:unit": "jest --env node",
    "test:cover": "yarn test:unit --coverage",
    "test:watch": "jest --env node --watch",
    "test:e2e": "npm-run-all test:e2e:*",
    "test:e2e:rollup-plugin-vue-i18n": "yarn build:example:rollup-plugin-vue-i18n && cd ./packages/rollup-plugin-vue-i18n && jest --runInBand --config ./jest.e2e.config.js",
    "test:e2e:vite-plugin-vue-i18n": "cd ./packages/vite-plugin-vue-i18n && jest --runInBand --config ./jest.e2e.config.js",
    "test:e2e:vue-i18n-loader": "yarn build:vue-i18n-loader && cd ./packages/vue-i18n-loader && jest --runInBand --config ./jest.e2e.config.js"
  },
  "devDependencies": {
    "@rollup/pluginutils": "^4.1.0",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@types/debug": "^4.1.5",
    "@types/eslint": "^7.2.6",
    "@types/eslint-visitor-keys": "^1.0.0",
    "@types/jest": "^26.0.19",
    "@types/jsdom": "^16.2.5",
    "@types/node": "^14.14.17",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "@vue/compiler-sfc": "^3.0.11",
    "debug": "^4.1.1",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^7.8.0",
    "eslint-plugin-vue-libs": "^4.0.0",
    "jest": "^26.6.3",
    "jest-puppeteer": "^5.0.0",
    "jest-watch-typeahead": "^0.6.1",
    "jsdom": "^16.4.0",
    "@kazupon/lerna-changelog": "^4.1.0",
    "npm-run-all": "^4.1.5",
    "opener": "^1.5.2",
    "puppeteer": "^9.0.0",
    "rollup": "^2.35.1",
    "rollup-plugin-vue": "^6.0.0",
    "serve": "^11.3.2",
    "ts-jest": "^26.5.0",
    "typescript": "^4.2.3",
    "typescript-eslint-language-service": "^4.1.3",
    "vue": "3.0.11",
    "vue-i18n": "^9.1.6",
    "@vitejs/plugin-vue": "^1.2.1",
    "vite": "^2.1.5",
    "@intlify/runtime": "^9.1.1",
    "@types/loader-utils": "^2.0.0",
    "@types/memory-fs": "^0.3.2",
    "@types/webpack": "^4.41.26",
    "@types/webpack-merge": "^4.1.5",
    "babel-loader": "^8.1.0",
    "memory-fs": "^0.5.0",
    "source-map": "^0.6.1",
    "vue-loader": "^16.2.0",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2"
  },
  "private": true,
  "changelog": {
    "labels": {
      "Type: Feature": ":star: Features",
      "Type: Bug": ":bug: Bug Fixes",
      "Type: Security": ":lock: Security Fixes",
      "Type: Performance": ":chart_with_upwards_trend: Performance Fixes",
      "Type: Improvement": ":zap: Improvement Features",
      "Type: Breaking": ":boom: Breaking Change",
      "Type: Deprecated": ":warning: Deprecated Features",
      "Type: I18n": ":globe_with_meridians: Internationalization",
      "Type: A11y": ":wheelchair: Accessibility",
      "Type: Documentation": ":pencil: Documentation"
    }
  },
  "workspaces": [
    "packages/*"
  ]
}
